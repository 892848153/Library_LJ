<?xml version="1.0" encoding="utf-8"?>
<com.lj.library.widget.renderperform.CustomFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                       android:layout_width="match_parent"
                                                       android:layout_height="match_parent">

    <com.lj.library.widget.renderperform.CustomFrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:orientation="horizontal">

        <com.lj.library.widget.renderperform.CustomTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:text="text1"/>

        <com.lj.library.widget.renderperform.CustomButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="text1"/>

    </com.lj.library.widget.renderperform.CustomFrameLayout>

</com.lj.library.widget.renderperform.CustomFrameLayout>

